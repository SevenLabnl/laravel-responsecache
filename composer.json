{
    "name": "7lab/laravel-responsecache",
    "description": "Cache responses in a Laravel application",
    "keywords": [
        "7lab",
        "laravel-responsecache",
        "laravel",
        "responsecache",
        "response",
        "cache"
    ],
    "homepage": "https://github.com/7lab/laravel-responsecache",
    "license": "MIT",
    "authors": [
        {
            "name": "Joey Houtenbos",
            "email": "joey@7lab.nl",
            "homepage": "https://www.7lab.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "SevenLab\\ResponseCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SevenLab\\ResponseCache\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SevenLab\\ResponseCache\\ResponseCacheServiceProvider"
            ],
            "aliases": {
                "ResponseCache": "SevenLab\\ResponseCache\\Facades\\ResponseCache"
            }
        }
    }
}
